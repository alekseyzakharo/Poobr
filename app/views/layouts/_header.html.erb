

  <ul class="navbar">
    <li style="float: left"><%= image_tag("logo---.png", width:'100em', :alt => "logo") %></li>
    <% if logged_in? %>
    <li class="dropdown">
        <a href="#" class="dropdown-toggle" data-toggle="dropdown">
           Logged In As: <%=current_user.name%>
        <b class="caret"></b>
        </a>
        <ul class="dropdown-menu dropdown-menu-right">
            <li><%= link_to "Postings", postings_url %></li><br>
            <li><%= link_to "Profile", current_user %></li><br>
            <li><%= link_to "Settings", edit_user_path(current_user)%></li><br>
            <li class="divider"></li>
            <li><%= link_to "Log out", logout_path, method: "delete" %></li>
        </ul>
    </li>

      <% if current_user.level == 0 %>
        <li class="userlist"><%= link_to "Users list", users_path %></li>
      <% end %>
      <li><%= link_to 'Home', root_url %></li>
    <% else %>
      <li><%= link_to "Log in", login_path %></li>
      <li><%= link_to "Create Account", new_user_path %></li>
      <li><%= link_to 'Home', root_url %></li>
    <% end %>

  </ul>
