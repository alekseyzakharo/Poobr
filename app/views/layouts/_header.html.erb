

  <ul class="navbar">
    <li style="float: left"><%= image_tag("logo---.png", width:'100em', :alt => "logo") %></li>
    <% if logged_in? %>
    <li class="logout"><%= link_to "Log out", logout_path, method: "delete" %></li>
      <li><a> Logged in as <%= current_user.name %> </a></li>
      <li><%= link_to "Profile", current_user %></li>
      <li><%= link_to "Postings", postings_url %></li>
      <li><%= link_to 'Home', root_url %></li>
      <% if current_user.level == 0 %>
        <li class="userlist"><%= link_to "Users list", users_path %></li>
      <% end %>
    <% else %>
      <li><%= link_to "Log in", login_path %></li>
      <li><%= link_to "Create Account", new_user_path %></li>
      <li><%= link_to 'Home', root_url %></li>
    <% end %>
  </ul>
